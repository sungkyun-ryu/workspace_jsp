	<%
	
	if(가로로) {
		for (int dan=2 ; dan < 10 ; dan++) {
			for (int i=1; i < 10 ; dan++) {
				out.print(dan + " * "+ i + " = " );
				out.print(multi(dan, i) + "&nbsp");
			} out.print("<br/>");			
		}
	}
	
	%>

